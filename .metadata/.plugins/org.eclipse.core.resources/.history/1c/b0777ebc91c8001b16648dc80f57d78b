package classe;

public class DataTeste {
	
	public static void main(String[] args) {
		
		Data d1 = new Data(01 , 01 , 1970);
		Data d2 = new Data ();
		d1.ano = 2021;
		
		System.out.printf("%d/%d/%d " , d1.dia, d1.mes, d1.ano);
		
		System.out.printf("\n%d/%d/%d ", d2.dia , d2.mes ,d2.ano);
		
	}

}
