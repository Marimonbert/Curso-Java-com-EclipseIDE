package classe;

public class ProdutoTeste {

	public static void main(String[] args) {
		
		Produto p1 = new Produto("Notebook" , 4.500 , 0.25);

		Produto p2 = new Produto("Geladeira" , 3.200 , 0.30);
	
		
		System.out.println("Nome do Produto: " + p1.nome + "\nPreço do produto: " +
		p1.preco + "\nDesconto: " + p1.desconto);
		
		System.out.println("Nome do Produto: " + p2.nome + "\nPreço do produto: " +
		p2.preco + "\nDesconto: " + p2.desconto);
		
		
	}
	
}
